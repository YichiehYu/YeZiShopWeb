<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wujiuye.yezishop.bean.MerchandiseImagesBean" table="com_wujiuye_yezishop_merchandise_images">
    
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        
        <property name="merchId" type="int">
            <column name="merch_id" />
        </property>
        
        <property name="bigPath" type="java.lang.String">
            <column name="bigpath" />
        </property>
        
        <property name="newPath" type="java.lang.String">
            <column name="newpath" />
        </property>
        
    </class>
</hibernate-mapping>
