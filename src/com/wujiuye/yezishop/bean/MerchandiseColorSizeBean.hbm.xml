<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

    <class name="com.wujiuye.yezishop.bean.MerchandiseColorSizeBean" table="com_wujiuye_yezishop_mercchandise_colorsize">
    
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        
        <property name="merchId" type="int">
            <column name="merch_id" />
        </property>
        
        <property name="color" type="java.lang.String">
            <column name="color" />
        </property>
        
        <property name="size" type="java.lang.String">
            <column name="size" />
        </property>
        
    </class>
</hibernate-mapping>
