<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.wujiuye.yezishop.bean">

	<class name="HomeHeadNewsBean" table="com_wujiuye_yezishop_homehead_news">

		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>

		<property name="clickToUrl" type="java.lang.String">
			<column name="url" />
		</property>

		<property name="imageUrl" type="java.lang.String">
			<column name="image" />
		</property>
	</class>

</hibernate-mapping>
